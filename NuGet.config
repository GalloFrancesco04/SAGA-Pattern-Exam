<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
    <add key="local" value="./nuget-packages" />
    <add key="github" value="https://nuget.pkg.github.com/GalloFrancesco04/index.json" />
  </packageSources>
  <packageSourceMapping>
    <packageSource key="local">
      <package pattern="SaaS.*" />
    </packageSource>
    <packageSource key="github">
      <package pattern="GalloFrancesco04.*" />
    </packageSource>
    <packageSource key="nuget.org">
      <package pattern="*" />
    </packageSource>
  </packageSourceMapping>
</configuration>
